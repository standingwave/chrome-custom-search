{
  "manifest_version": 2,
  "name": "Custom Search Engine",
  "version": "0.1",
  "description": "Custom search engine button for your toolbar.",
  "icons": {"128": "icon.png"},
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "browseraction.png",
    "default_title": "Manage Custom Search Engine",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://localhost:5000/*",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  }
}